#admin_menu a:before
    font-family FontAwesome
    content '\f105'
    padding-right 8px

.videoWrapper
    text-align center
    position relative
    padding-bottom 58.75%
    padding-top 15px
    height 0
    margin 15px

.videoWrapper iframe, .videoWrapper object
    position absolute
    top 0
    left 0
    width 100%
    height 100%
    vertical-align middle